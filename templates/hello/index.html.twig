{% extends 'base.html.twig' %}

{% block title %} This is the new Title {% endblock %}

{% block body %}
    <div>
        {% if messages|length > 0 %}
            {% for message in messages|slice(0, limit) %}
                <div>
                    {{ message.message }}
                </div>
                {{ include('hello/_message.html.twig', {message: message}) }}
            {% endfor %}
        {% else %}
            <div> Nothing to show</div>
        {% endif %}
    </div>
{% endblock %}